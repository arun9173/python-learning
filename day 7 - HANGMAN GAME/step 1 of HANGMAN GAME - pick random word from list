################################## PRACTICE CODE 1 ###################################

import random
word_list = ["aardvark", "baboon", "camel"]
chosen_word = random.choice(word_list)
print(chosen_word)

#################################### PRACTICE CODE 2 #################################

import random
word_list = ["aardvark", "baboon", "camel"]
rand_num = random.randint(0,len(word_list)-1)
print(word_list[rand_num])

